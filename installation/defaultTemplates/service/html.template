{%extends "components/includes/base.html"%}
{%block content%}

<div class="container">
<h1>Default service</h1>
<div class="row">

	<div class="table-responsive">
		<table class="table table-hover">
			<thead>
				<tr>
					<th>S</th><th>P</th><th>O</th>
				</tr>
			</thead>
			<tbody>
				{%for row in queries.main%}
				<tr>
					<td>{{row.s.value}}</td><td>{{row.p.value}}</td><td>{{row.o.value}}</td>
				</tr>
				{%endfor%}
			</tbody>
		</table>
	</div>

</div>
	
</div>
{%endblock%}
